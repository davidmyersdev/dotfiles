[commit]
  gpgsign = true
[core]
  editor = vim
  ignorecase = false
  excludesfile = /Users/david/git/ignore
  autocrlf = input
[diff]
  external = difft
  tool = difftastic
[difftool]
  prompt = false
[difftool "difftastic"]
  cmd = difft "$LOCAL" "$REMOTE"
[filter "lfs"]
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
[init]
  defaultBranch = main
[interactive]
  diffFilter = diffr --colors added:none:foreground:0,0xdd,0 --colors refine-added:none:foreground:0,0xdd,0:background:0x10,0x30,0x10 --colors removed:none:foreground:0xdd,0,0 --colors refine-removed:none:foreground:0xdd,0,0:background:0x30,0x10,0x10
[gpg]
  program = gpg
[pager]
  difftool = true
[pull]
  rebase = true
[rebase]
  autosquash = true
[tag]
  gpgsign = true
